module Fib;

type <java> "java.lang.Integer" from "rt.jar" as int;


global protocol Adder(role C, role S)
{
  HELLO(u:int) from C to S; @"u < 3"

  choice at C
  {
    ADD(y:int) from C to S;
    RES(v:int) from S to C; @"v > 0"
    do Adder(C, S);
  }
  or
  {
    BYE() from C to S;
    BYE() from S to C;
  }
}
