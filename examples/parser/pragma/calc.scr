
(*# SamplePragma #*)

global protocol Calc(role S , role C ) {
  rec Loop {
    choice at C {
      sum (int, int) from C to S ;
      result (int) from S to C ;
      continue Loop ;
    } or {
      multiply (int, int) from C to S ;
      result (int) from S to C ;
      continue Loop ;
    } or {
      quit () from C to S ;
      terminate () from S to C ;
    }
  }
}
